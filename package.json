{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "description": "Drag and drop so simple it hurts - Second edition",
  "homepage": "https://toofff.github.io/dragula2",
  "repository": {
    "type": "git",
    "url": "https://github.com/toofff/dragula2.git"
  },
  "license": "MIT",
  "author": "Prou Yann <toofff.development@gmail.com>",
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean && rimraf \"**/{.turbo,dist,node_modules}\" --glob --no-interactive",
    "preinstall": "npx only-allow pnpm",
    "lints": "turbo run lint",
    "lints:fix": "turbo run lint:fix",
    "prepare": "husky install",
    "start": "turbo run start",
    "tests": "turbo run test",
    "tests:ci": "turbo run test:ci"
  },
  "devDependencies": {
    "@commitlint/cli": "17.5.1",
    "@commitlint/config-conventional": "17.4.4",
    "husky": "8.0.3",
    "lint-staged": "13.2.0",
    "rimraf": "4.4.1",
    "sort-package-json": "2.4.1",
    "turbo": "1.9.1"
  },
  "packageManager": "pnpm@8.1.0",
  "engines": {
    "pnpm": ">=8.1.0"
  }
}
